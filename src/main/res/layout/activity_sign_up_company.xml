<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_background"
    tools:context=".login.SignUpCompany">
    <ImageView
        android:id="@+id/iv_logo"
        android:layout_width="@dimen/_150dp"
        android:layout_height="50dp"
        android:layout_marginLeft="@dimen/_20dp"
        android:layout_marginTop="@dimen/_30dp"
        android:src="@drawable/kamdo_logo" />
    <androidx.core.widget.NestedScrollView
        android:id="@+id/nsv_reg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/iv_logo"
        android:layout_marginTop="@dimen/_15dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:paddingBottom="@dimen/_30dp"
            android:layout_marginBottom="@dimen/_20dp"
            >
            <LinearLayout
                android:id="@+id/ll_contain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_des"
                android:layout_marginLeft="@dimen/_20dp"
                android:layout_marginTop="@dimen/_10dp"
                android:layout_marginRight="@dimen/_20dp"
                android:background="@drawable/round_shape"
                android:orientation="vertical"
                android:padding="@dimen/_20dp">
                <TextView
                    android:id="@+id/tv_registration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:text="@string/registration"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_20dp"
                    android:textStyle="bold"/>
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/iv_profile"
                    android:layout_width="@dimen/_60dp"
                    android:layout_height="@dimen/_60dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/_5dp"
                    android:src="@drawable/no_profile"
                    />
                <EditText
                    android:id="@+id/edt_Cname"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/cname"
                    android:inputType="text"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_gstno"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/gstno"
                    android:inputType="text"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_mobileno"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/mobile"
                    android:inputType="phone"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_address"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/address"
                    android:inputType="text"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_faddress"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/faddress"
                    android:inputType="text"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_description"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/adddes"
                    android:inputType="text"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_password"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/password"
                    android:inputType="textPassword"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <EditText
                    android:id="@+id/edt_conpassword"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_15dp"
                    android:background="@drawable/round_shape_edittext"
                    android:hint="@string/conpassword"
                    android:inputType="textPassword"
                    android:imeOptions="actionNext"
                    android:paddingLeft="@dimen/_20dp"
                    android:paddingRight="@dimen/_20dp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="13dp" />
                <Button
                    android:id="@+id/btn_login"
                    android:layout_width="@dimen/_180dp"
                    android:layout_height="@dimen/_40dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_20dp"
                    android:background="@drawable/button_background"
                    android:text="@string/signin"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_15dp" />
            </LinearLayout>
            <TextView
                android:id="@+id/tv_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_35dp"
                android:textColor="@color/black"
                android:text="@string/already_login"
                android:layout_below="@id/ll_contain"/>
        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>
</RelativeLayout>